@import "stars/stars.wxss";
@import "movelist/movelist-template.wxss";
@import "move/move-template.wxss";
/* 搜索框样式 */
.search {
  background-color: #f2f2f2;
  height: 80rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}

.search-img {
  margin: auto 0 auto 20rpx;
}

.search input {
  height: 100%;
  width: 600rpx;
  margin-left: 20px;
  font-size: 28rpx;
}

.placeholder {
  font-size: 14px;
  color: #d1d1d1;
  margin-left: 20rpx;
}
.xx-img{
    height: 30rpx;
    width: 30rpx;
     margin:20rpx 0 auto 10rpx; 
     vertical-align: middle
     
}
.search_btn{
  display: inline-block;
  width: 100rpx;
  text-align: center;

}
